<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ru.fefu.helloworld">

    <application
        android:icon="@drawable/group_8"
        android:label="@string/app_name"
        android:roundIcon="@drawable/group_8"
        android:supportsRtl="true"
        android:theme="@style/Theme.StartScreen">

        <activity
            android:name=".StartScreenActivity"
            android:label="Start Screen"
            android:exported="true"
            android:theme="@style/Theme.StartScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AuthActivity"
            android:label="Auth Screen"
            android:exported="true" />

        <!-- Register screen -->
        <activity
            android:name=".RegisterActivity"
            android:label="Register Screen"
            android:exported="true" />

    </application>
</manifest>
